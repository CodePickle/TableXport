{
   "background": {
      "scripts": [ "jquery-2.1.1.min.js", "tablexport.js", "background.js" ],
      "persistent": false
   },
   "browser_action": {
      "default_icon": "img/48.png"
      },
  "content_scripts": [ {
      "js": [ "jquery-2.1.1.min.js", "tablexport.js" ],
      "matches": [ "http://*/*" ]
   } ],   
   "icons": {
      "128": "img/128.png",
      "48": "img/48.png",
      "16": "img/16.png"
   },
   "manifest_version": 2,
   "name": "TableXport",
   "description": "Create CSV from HTML Table",
   "version": "1.0",
   "permissions": [ "activeTab" ]
}
